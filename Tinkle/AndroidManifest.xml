            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.hitchlab.tinkle.appevent.MemberActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.hitchlab.tinkle.appevent.EventInviteActivity"
