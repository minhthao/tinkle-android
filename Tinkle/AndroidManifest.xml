        android:protectionLevel="signature" />

    <uses-permission android:name="com.hitchlab.tinkle.permission.C2D_MESSAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@android:style/Theme.NoTitleBar" >
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyC01BIN66zffWg_AvCaNH7mmKKVTjemYwM" />

        <activity
            android:name="com.hitchlab.tinkle.SplashActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hitchlab.tinkle.DeepLinkSplashActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="frenvent" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hitchlab.tinkle.LoginActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.hitchlab.tinkle.MainActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.hitchlab.tinkle.MenuHistoryActivity"
