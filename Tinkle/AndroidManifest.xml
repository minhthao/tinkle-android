        <service android:name="com.hitchlab.tinkle.service.CheckNotificationService" />
        <service android:name="com.hitchlab.tinkle.service.CheckSyncCalendarService" />
        <service android:name="com.hitchlab.tinkle.service.CheckUpdatingService" />
